<h1>new person</h1>
<%= form_for :person, url: person_path do |f| %>
  <p>
    <%= f.label :number %><br>
    <%= f.text_field :number %>
  </p>
 
  <p>
    <%= f.label :long %><br>
    <%= f.text_area :long %>
  </p>

  <p>
    <%= f.label :lat %><br>
    <%= f.text_area :lat %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>