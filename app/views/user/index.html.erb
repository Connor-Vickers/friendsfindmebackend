<h1>Listing Users</h1>
 
<table>
  <tr>
    <th>number</th>
    <th>lat</th>
    <th>long</th>
    <th>time modified</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.number %></td>
      <td><%= user.lat %></td>
      <td><%= user.long %></td>      
      <td><%= user.updated_at %></td>
    </tr>
  <% end %>
</table>
<br>
<%= DateTime.now %>